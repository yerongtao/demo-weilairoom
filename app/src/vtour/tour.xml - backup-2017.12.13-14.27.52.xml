<!-- 地图部分 url对应的是地图本身的路径，按照自己的文件修改url中数值 -->
<layer name="map" url="skin/camicon_map.png" keep="true" handcursor="false" 
	   capture="false"
	   align="lefttop"
	   x="20"
       y="0"
	   scale="0.25" 
	   scalechildren="true"
	   onclick="openmap();"
	   />
 <layer name="map_button" url="skin/map_2.png" keep="true" handcursor="false" 
		   capture="false"
	       align="lefttop"
		   state="opened"
		   x="0" 
		   y="2" 
		   ox="0" 
		   oy="0" 
		   maskchildren="false" 
	       scale="0.25" scalechildren="true"
		   onclick="if(layer[map_button].state=='opened',
			
				    
		             tween(layer[map].x,-200,0.5,easeOutElastic); 
					 tween(layer[map_container].x,-200,0.5,easeOutElastic); 
					 set(layer[map_button].url,./vtour/skin/map_1.png);
		             set(layer[map_button].state,closed);
					 tween(layer[map_button].alpha,0.5);
					 set(layer[map_button].onhover,tween(layer[map_show].alpha,1));
					 set(layer[map_button].onout,tween(layer[map_show].alpha,0.5)),
                     
					 
					 tween(layer[map].x,20,0.5,easeOutElastic); 
					 tween(layer[map_container].x,20,0.5,easeOutElastic);            
	                 set(layer[map_button].url,./vtour/skin/map_2.png);
	                 set(layer[map_button].state,opened);
					 tween(layer[map_button].alpha,1);
					 set(layer[map_button].onhover,NULL);
					 set(layer[map_button].onout,NULL);
					 
					 ); "
	       />
	<!-- 地图打开部分 - 缩放到100% (如果屏幕小的话则可能更小) -->
	<action name="openmap">
		set(layer[map].onclick, closemap(); );
		layer[map].changeorigin(center,center);
 
		set(bigscale,1);
		if(layer[map].imagewidth GT stagewidth, div(bigscale, stagewidth, layer[map].imagewidth); );
 
		tween(layer[map].x, 0);
		tween(layer[map].y, 0);
		tween(layer[map].scale, get(bigscale));
	</action>
 
	<!-- 地图关闭部分 - 缩放到自身尺寸的四分之一 -->
	<action name="closemap">
		set(layer[map].onclick, openmap(); );
		layer[map].changeorigin(lefttop,lefttop);
 
		tween(layer[map].x, 20);
		tween(layer[map].y, 0);
		tween(layer[map].scale, 0.25);
	</action>
 
 
	<!-- 地图点图像部分 url可以设置 用style来统一处理 所有layer都载入了一个名为mapspot的style，注意这里spot是由0开始，而不是由1开始的，因此在一般情况下，地图点与场景一一对应，而场景的index是从0开始的，所以我们可以利用这一点提高代码的可读性 -->
	<style name="mapspot" keep="true" url="skin/camicon.png" parent="map" align="lefttop" edge="center" scale.mobile="2" />
 
	<layer name="spot0" style="mapspot" x="300" y="210" zorder="1" onclick="mapspot_loadscene();" />
	<layer name="spot1" style="mapspot" x="400" y="160" zorder="1" onclick="mapspot_loadscene();" />
	<layer name="spot2" style="mapspot" x="470" y="220" zorder="1" onclick="mapspot_loadscene();" />
 
	<!-- 当点击地图点时执行的动作 -->
	<action name="mapspot_loadscene">
		subtxt(spotid,get(name),4,2);
 
		if(layer[map].scale GT 0.25,
			<!-- 当地图放大时需要执行更多特殊的动作，缩小地图等 -->
			set(layer[map].enabled, false);
			tween(layer[map].alpha, 0.0, 0.25, default,
			
			if(spotid != scene[get(xml.scene)].index,  
			  loadscene(get(scene[get(spotid)].name),null,MERGE,BLEND(1)); 
			  );
				
				set(layer[map].onclick, openmap(); );
				layer[map].changeorigin(lefttop,lefttop);
 
				set(layer[map].x, 20);
				set(layer[map].y, 0);
				set(layer[map].scale, 0.25);
 
				set(events[sceneload].onloadcomplete,
					delayedcall(1,
						tween(layer[map].alpha, 1.0, 0.5, default, set(layer[map].enabled, true); );
					  );
				  );
			  );
		  ,
			<!-- 小地图时只需要载入场景 -->
			if(spotid != scene[get(xml.scene)].index,  
			  loadscene(get(scene[get(spotid)].name),null,MERGE,BLEND(1)); 
			  );
				
		  );
	</action>
 
	<!-- 激活点图像 (覆盖在普通的地图点上) -->
	<layer name="activespot" url="skin/camicon_red.png" keep="true"
	       align="lefttop" zorder="2" scale.mobile="2"
